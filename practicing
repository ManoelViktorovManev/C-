            Console.WriteLine("Wuwedi duljinata na matricata");
            a = int.Parse(System.Console.ReadLine());
            b = int.Parse(System.Console.ReadLine());
            int[,] matric = new int[a, b];
            int boza = 1;
            for (int i = 0; i < b; i++)
            {
                
