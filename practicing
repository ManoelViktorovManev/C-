                    for (int k = 0; k < a / 2; k++)
                    {


                        int c = matric[i, k];
                        matric[i, k] = matric[i, matric.GetLength(0) - 1 - j];
                        matric[i, matric.GetLength(0) - 1 - j] = c;
                        j++;
                    }
                }

                


                
